{
  "author": "Tong",
  "name": "electron-react-demo",
  "version": "0.1.0",
  "description": "first electron build",
  "homepage": "./",
  "private": true,
  "main": "public/electron.js",
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.2"
  },
  "devDependencies": {
    "electron": "^18.3.7",
    "electron-builder": "^21.2.0"
  },
  "scripts": {
    "r-start": "react-scripts start",
    "r-build": "react-scripts build",
    "r-test": "react-scripts test",
    "r-eject": "react-scripts eject",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "electron-pack": "build --em.main=build/electron.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.cilent.app.electronbuild",
    "productName": "electron-build",
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "icon": "icons/Mushroom.ico"
    },
    "win": {
      "icon": "icons/Mushroom.ico"
    },
    "linux": {
      "icon": "icons/Mushroom.ico"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ]
  }
}
